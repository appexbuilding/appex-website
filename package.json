{
  "name": "appex-boilerplate-hugo-uikit",
  "version": "1.0.0",
  "description": "Built by Sam Brunno, This is a webpack boilerplate for static websites that has all the necessary modern tools and optimizations built-in.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "lint:styles": "stylelint \"src/**/*.css\"",
    "start": "run-p start:**",
    "start:hugo": "hugo -d ../dist -s site -vw",
    "start:webpack": "webpack-dev-server --open --config webpack.dev.js",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "preview:webpack": "npm run start:webpack",
    "prebuild": "rimraf dist",
    "build": "npm run build:webpack && npm run build:hugo",
    "build:algolia": "npm run build:webpack && npm run build:hugo && npm run algolia",
    "build:preview": "npm run build:webpack && npm run build:hugo:preview",
    "build:hugo": "hugo -d ../dist -s site -v --minify",
    "build:hugo:preview": "npm run build:hugo -- -D -F",
    "build:lambda": "netlify-lambda build backend",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js --hot --inline",
    "algolia": "atomic-algolia"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "keywords": [
    "appex",
    "boilerplate"
  ],
  "author": "sammyb",
  "license": "ISC",
